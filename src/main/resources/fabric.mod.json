{
	"schemaVersion": 1,
	"id": "astikorcarts",
	"version": "${version}",
	"name": "AstikorCarts",
	"description": "A Fabric mod port for Minecraft: Java Edition which adds carts for travel, shipping and farming.",
	"authors": [
		"MennoMax",
		"paul101",
		"jmb19905"
	],
	"contact": {
		"sources": "https://github.com/jmb05/astikor-carts"
	},
	"license": "MIT",
	"icon": "assets/astikorcarts/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.jmb19905.astikorcarts.AstikorCarts"
		],
		"client": [
			"net.jmb19905.astikorcarts.client.AstikorCartsClient"
		]
	},
	"mixins": [
		"astikorcarts.mixins.json",
		{
			"config": "astikorcarts.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	},
	"accessWidener": "astikorcarts.accesswidener"
}